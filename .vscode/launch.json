  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "train",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/vmf_contact_main/train.py",
      "console": "integratedTerminal",
      "args": [
        "--scale", "1.75",
        "--batch-size", "2",
        "--embedding_dim", "240",
        //"--pcd_with_rgb",
        "--learning_rate", "1e-5",
        "--prob_baseline", "lh",
        "--flow_layers", "8",
        
        "--point_backbone", "pointnext-b",
        "--ckpt","epoch=38-step=29874",
        "--devices", "4",
        "--debug",

        //"--eval",
        //"--run-finetuning",
        "--flow_finetune", "0",
        "--num_workers", "10"
      ],
      "env": {
        //"TORCH_USE_CUDA_DSA": "1",
        //"CUDA_LAUNCH_BLOCKING": "1",
        //"CUDA_VISIBLE_DEVICES": "4"
      }
    //"justMyCode": false
    },
    {
      "name": "flow",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/vmf_contact/nn/uncertainty_estimator.py",
      "console": "integratedTerminal",
      "args": [],
      "env": {
        "TORCH_USE_CUDA_DSA": "1",
        //"CUDA_LAUNCH_BLOCKING": "1",
        "CUDA_VISIBLE_DEVICES": "0"
      }
    }
  ]
}
